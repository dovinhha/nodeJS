extends ../layout/common
block content
  .row
    each val in products
      .col-sm-6.col-md-4.col-lg-3
        .card.mb-3
          img.card-img-top(src=val.product.image)
          .card-body
            h5.card-title=val.product.name
            p.card-text=val.product.desciption
            h5.card-title Amount
            p.card-text= val.amount